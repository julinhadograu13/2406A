     class Animal(var especie: String, //atributos que todo animalzin deve ter
              var raca: String,
              var idade: Int,
              var dieta: String,
              var porte: String){   
    
    //estrutura de um método:
    //fun (nomeDoParametro: tipodeparametro){
    //ação
    //}
​
     fun comer(alimento: String){ 
         println("$especie comendo $alimento, yumm")
                
}
     fun correr(velocidade: Double, lugar: String){
         println("$especie correndo em uma velocidade de $velocidade km/h no $lugar")
         
     }
