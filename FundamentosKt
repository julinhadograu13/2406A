//POO
  //
  //  Programação Orientada a Objetos
  //  
  //  Objetivo: Simular atividade e situação do mundo real
  //    
  //  Objeto - elementos que podem interagir com um sistema   ex.:pé
  //  Classe - determinação de um objeto  ex.:membros do corpo 
  //  Atributos - caracteristicas, propriedades, parâmetros  ex.:tamanho
  //  metodos - atividades, ações, que o obj pode realizar  ex.:chutar
  //                  
  //  Herança - Atributos de um obj que são HERDADOS de uma classe
  //  
  //  
  ---------------------------------------
  
  fun main() {
  
 var pessoa1 = Pessoa() // instanciamos um objeto 

 println(pessoa1.idioma) //Exibir um atributo de um objeto
 println(pessoa1.nome)
 pessoa1.nome = "Julia" //alterar m valor de um atributo de um objeto
 println(pessoa1.nome + " , "+pessoa1.idade)
 
}
  

class Pessoa {
    
  var nome = "Julia"  
  var idade = 16
  var nacionalidade = "Brasileira"
  var idioma= "Português"
  var profisso = "Desempregada" 
  
    
                                           
   
}
----------------------------------------------------
fun main() {
    /* 
   var pessoa1 = Pessoa() //instanciamos um obj   
   
   println(pessoa1.idioma) //Exibir um atributo de um obj
   println(pessoa1.nome)   
   pessoa1.nome = "Rayane"
   println(pessoa1.nome + ", "+pessoa1.idade)
    
    //Exercícios -> Faça uma apresentação sua ultilizando a classe pessoa() já criada e ultilizando pelo 
    //menos, todos os atribulados que existem na classe    
 */ 
    
     var pessoa3 = Pessoa()   
     var animal1 = Animal("Cachorro", "Pinscher", 7, "Chinelo","grande")
     var animal2 = Animal("Girafa" , "Giraffa tippelskirchi" , 25 ,"folhas" ,"grande")
