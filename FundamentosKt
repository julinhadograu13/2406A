}else if(coracoes == 3){
    println("cuidado com a sua saúde")
}else if(coracoes == 2){
    println("vá ao médico !!!!!!")
}else if(coracoes == 1){
    println("seu estado é critico!!!!!!")
}else{
    println("novo reforço do vasco")
}
*/

//1 -
// 
// 
var contas
var salario = 3000
if(salario = 400 > contas){
    println("salario é maior que as contas, parabéns")
}else{
    println("as contas estão maior que o alugel")
}
 
//2 -


var fatiasdepizza = 8
if(fatiasdepizza > 8)
    println("bora comer,a pzza acabou de chegar")
}else if{(fatiasdepizza == 6){
    println("ainda estou com fome, vou comer")
}else if{(fatiasdepizza == 4){  
    println("já comi batante, mais ainda aguento mais")
}else if{(fatiasdepizza == 1){
    println("estou triste por te comido de mais")
}else{
    println("a pizza acabou")
}
    
    
}
